 <link name="scanner">
   <visual>
	<geometry>
		<box size="0.015 0.045 0.015"/>
	</geometry>
       <origin rpy="0 0 0" xyz="0 0 -0.05"/>
	<material name="blue">
		<color rgba="1 0 0 1"/>
	</material>
   </visual>
  </link>

  <joint name="base_to_scanner" type="fixed">
     <parent link="base_link"/>
     <child link="scanner"/>
     <origin xyz="0.105 0 0.22" rpy="0 0 0"/>
  </joint>



</robot>
