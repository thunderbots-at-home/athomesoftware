<?xml version="1.0"?>
<robot name="thunderbot">

  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.61905 0.61905 0.40476"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.311525"/>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
  </link>

  <link name="lwheel">
    <visual>
      <geometry>
        <cylinder length="0.008" radius="0.0762"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </visual>
  </link>
  
  <joint name="base_to_lwheel" type="fixed">
    <parent link="base_link"/>
    <child link="lwheel"/>
    <origin xyz="0 0.115 0.046" rpy="1.5708 0 0"/>
  </joint>
  
  <link name="rwheel">
    <visual>
      <geometry>
        <cylinder length="0.008" radius="0.0"/>
